{
  "AppConfig": {
    "allowedTokens": [],
    "OdruOptions": {
      "ClientId": "",
      "ClientSecret": "",
      "TenantId": "",
      "UserName": "",
      "DriveId": "",
      "SiteName": "",
      "RootDirectory": ""
    },
    "NotificationOptions": {
      "Endpoints": [],
      "ReferralUrlBase": "",
      "AllowedHostEmails": []
    },
    "SlackApiOptions": {
      "BotUserToken": ""
    }
  },
    "Logging": {
      "LogLevel": {
        "Default": "Debug",
        "Microsoft": "Debug",
        "Microsoft.Hosting.Lifetime": "Warning"
      }
    },
    "AllowedHosts": "*",
    "Serilog": {
      "MinimumLevel": "Debug",
      "WriteTo": [
        {
          "Name": "Async",
          "Args": {
            "configure": [
              {
                "Name": "Console",
                "Args": {
                  "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
                  "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:j}{NewLine}{Properties:j}{NewLine}{Exception}"
                }
              }
            ]
          }
        }
      ],
      "Enrich": [
        "FromLogContext",
        "WithMachineName",
        "WithExceptionDetails"
      ],
      "Properties": {
        "ApplicationName": "ZoomFileManager",
        "Environment": "Prod"
      }
    }
  }

