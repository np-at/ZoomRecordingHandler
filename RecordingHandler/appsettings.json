{
  "$schema": "appsettings.schema.json",
  "AppConfig": {
    "ReceiverEndpointConfigs": [],
    "BaseReceiverConfig": {
      "BaseRouteTemplate": "api/[controller]"
    },
    "AllowedTokens": [],
    "UploadConfigs": [],
    "UploadTargets": []
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Warning",
      "Microsoft.Extensions.Http.DefaultHttpClientFactory": "Warning"
    }
  },
  "Serilog": {
    "Using": [
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Warning",
        "Microsoft.Extensions.Http.DefaultHttpClientFactory": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console",
              "Args": {
                "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
                "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:j}{NewLine}{Properties:j}{NewLine}{Exception}"
              }
            }
          ]
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithExceptionDetails"
    ],
    "Properties": {
      "ApplicationName": "ZoomFileManager",
      "Environment": "Prod"
    }
  },
  "AllowedHosts": "*"
}
